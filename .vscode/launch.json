{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run as root",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}/bin/harpoon",
            "asRoot": true,
            "console": "integratedTerminal",
            "args": ["capture", 
                "-f", 
                "<function_list,...>", 
                "/path/to/command",
                "args"
            ]
        }
    ]
}
